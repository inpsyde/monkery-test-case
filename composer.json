{
    "name": "inpsyde/monkery-test-case",
    "description": "Composition package of brain/monkey and mockery/mockery with a custom test case class for PHPUnit",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Inpsyde GmbH",
            "role": "Company",
            "homepage": "http://inpsyde.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "brain/monkey": "^2.0",
        "mockery/mockery": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 | ^9.0"
    },
    "conflict": {
        "phpunit/phpunit": "<8"
    },
    "suggest": {
        "phpunit/phpunit": "Unit testing framework. Usually installed globally."
    },
    "autoload": {
        "psr-4": {
            "MonkeryTestCase\\": "src/"
        }
    }
}
